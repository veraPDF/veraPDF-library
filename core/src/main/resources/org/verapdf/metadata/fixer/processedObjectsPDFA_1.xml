<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<appliedObjects>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>doCreationDatesMatch != false</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Title == null || Title == XMPTitle</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Author == null || (Author == XMPCreator &amp;&amp; XMPCreatorSize == 1)</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Subject == null || Subject == XMPDescription</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Keywords == null || Keywords == XMPKeywords</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Creator == null || Creator == XMPCreatorTool</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>Producer == null || Producer == XMPProducer</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>CosInfo</objectType>
        <test>doModDatesMatch != false</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>PDMetadata</objectType>
        <test>isCatalogMetadata == false || Filter == null</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>PDDocument</objectType>
        <test>containsMetadata == true</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>MainXMPPackage</objectType>
        <test>containsPDFAIdentification == true</test>
    </ruleDescription>
    <ruleDescription>
        <objectType>PDFAIdentification</objectType>
    </ruleDescription>
</appliedObjects>
